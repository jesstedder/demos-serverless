{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
      "frontend": {
        "matchCondition": {
          "route": "/{*restOfPath}"
        },
        "backendUri": "https://demoserverlessstorage.z21.web.core.windows.net/{restOfPath}"
      },
      "functions": {
        "matchCondition": {
          "route": "/api/{*restOfPath}"
        },
        "backendUri": "http://localhost:7071/api/{restOfPath}"
      }
    }
  }